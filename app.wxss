/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
view, navigator {
box-sizing: border-box;
padding: 0;
margin: 0;
}

.fsp{
   display:flex;
   justify-content:space-between;
   align-self:baseline;
}

/*居中 */
.center {
margin: 0 auto;
}

.clear:after {
    content: "";
    display: block;
    height: 0;
    clear: both;
}
.inner-container{
  padding:0 30rpx;
}
.clear {
    zoom: 1;
}
